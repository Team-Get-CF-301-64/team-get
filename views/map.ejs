<html>
  <head>
    <script src="https://api.mqcdn.com/sdk/mapquest-js/v1.3.2/mapquest.js"></script>
    <link type="text/css" rel="stylesheet" href="https://api.mqcdn.com/sdk/mapquest-js/v1.3.2/mapquest.css"/>

    <script type="text/javascript">
      window.onload = function() {
        L.mapquest.key = 'rG4CrkvfohnHOoaCGJ29POMC51kMr8ci';

        var map = L.mapquest.map('map', {
          center: [40.7128, -74.0059],
          layers: L.mapquest.tileLayer('map'),
          zoom: 13
        });

        let waypoints = <%- JSON.stringify(destinations.waypoints) %> 
        L.mapquest.directions().route({
          start: '<%= destinations.start %>',
          end: '<%= destinations.end %>',
          
          // waypoints: [ '366 Columbus Ave, New York, NY 10024', '881 7th Ave, New York, NY 10019']
          waypoints: waypoints

          
          

        });
      }
    </script>
  </head>

  <body style="border: 0; margin: 0;">
    <div id="map" style="width: 100%; height: 530px;"></div>
  </body>
</html>