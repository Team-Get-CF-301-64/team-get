<html>
	<head>
		<title>Telephasic by HTML5 UP</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="assets/css/main.css" />
	</head>
	<body class="homepage is-preload">
		<div id="page-wrapper">

			<!-- Header -->
			<div id="" style="  background: url(images/night.jpg) no-repeat center center; 
			-webkit-background-size: cover;
			-moz-background-size: cover;
			-o-background-size: cover;
			background-size: cover; height: 26em;">
					<div id="header" class="container">

				

						<!-- Nav -->
						<nav id="nav" style='margin-right: 0;'>
							<ul>
								<li>
									<a href="#">Menu</a>
									<ul>
										<li><a href="/">Home</a></li>
										<li><a href="/">Itinerary</a></li>
										<li><a href="/game">Travel Game</a></li>
										<li><a href="/aboutus">About Us</a></li>
										<li><a href="/music">Music</a></li>
									</ul>
								</li>
							
							</ul>
						</nav>
							<header class="major">
								<h2 style="color: white; padding-top: 8em;">This is where the music Lives</h2>
								<h2 style="color: white; padding-top: 12px;">Deezer Top 10</h2>								
							</header>
					</div>

					</div>

				<!-- Music 2 -->
				<div class="wrapper">
					<section class="container">
					                      

						<div class="row features">
                            
                            <% searchResults.forEach( albums => { %>
                        
								<section style="text-align: center;" class="col-5 col-12-narrower feature">
							
							
									<div class="image-wrapper first">
										<a href="#" class="image featured"><img src='<%= albums.cover_medium %>' alt="" /></a>
									</div>
								   <div>
									   <p class='musicfont'><%= albums.title %></p>
									   <p class='musicfont'><%= albums.artist %></p>
									   <p class='musicfont'>Position: <%= albums.position %></p>
									   <p class='musicfont'>link: <a href='<%= albums.link %>'><%= albums.link %></a></p>
	
									</div>
								</section>
                                <% }) %>
                                
                           
                            

						</div>
						<ul class="actions major">
							<li><a href="/" class="button">More</a></li>
						</ul>
					</section>
				</div>

			



</div>
		<!-- Scripts -->
			<script src="assets/js/jquery.min.js"></script>
			<script src="assets/js/jquery.dropotron.min.js"></script>
			<script src="assets/js/browser.min.js"></script>
			<script src="assets/js/breakpoints.min.js"></script>
			<script src="assets/js/util.js"></script>
			<script src="assets/js/main.js"></script>

	</body>
</html>