	<body class="homepage is-preload">
		<div id="page-wrapper">

			<!-- Header -->
				<div id="header-wrapper">
					<div id="header" class="container">

				

						<!-- Nav -->
							<nav id="nav">
								<ul>
									<li>
										<a href="#">Dropdown</a>
										<ul>
											<li><a href="/game">Travel Game</a></li>
                                            <li><a href="/">Home</a></li>
                                            <li><a href="/aboutus">About Us</a></li>
										</ul>
									</li>
								</ul>
							</nav>

					</div>

				<!-- Music 2 -->
				<div class="wrapper">
					<section class="container">
						<header class="major">
							<h2>This is where the music Lives</h2>
							<p>Elit aliquam vulputate egestas euismod nunc semper vehicula lorem blandit</p>
                        </header>
                        

						<div class="row features">
                            
                            <% searchResults.forEach( albums => { %>
                        
								<section style="text-align: center;" class="col-4 col-12-narrower feature">
							
							
									<div class="image-wrapper first">
										<a href="#" class="image featured"><img src='<%= albums.cover_medium %>' alt="" /></a>
									</div>
								   <div>
									   <p><%= albums.title %></p>
									   <p><%= albums.artist %></p>
									   <p>Position: <%= albums.position %></p>
	
									</div>
								</section>
                                <% }) %>
                                
                           
                            

						</div>
						<ul class="actions major">
							<li><a href="#" class="button">Elevate my awareness</a></li>
						</ul>
					</section>
				</div>

			



		</div>
</div>
