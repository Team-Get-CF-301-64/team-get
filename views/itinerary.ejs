    <link type="text/css" rel="stylesheet" href="https://api.mqcdn.com/sdk/mapquest-js/v1.3.2/mapquest.css"/>

  
	
	
		
	</head>


	<body class="homepage is-preload">


		<div id="page-wrapper">

			<!-- Header -->
            <div id="" style="  background: url(images/black.jpg) no-repeat center center; 
			-webkit-background-size: cover;
			-moz-background-size: cover;
			-o-background-size: cover;
			background-size: cover; height: 26em;">
					<div id="header" class="container">

				

						<!-- Nav -->
							<nav id="nav">
								<ul>
									<li>
										<a href="#">Dropdown</a>
										<ul>
											<li><a href="/">Home</a></li>
                                            <li><a href="/aboutus">About US</a></li>
										</ul>
									</li>
								</ul>
							</nav>
							<header class="major">
								<h2 style="color: white; padding-top: 8em;">Look at your plans</h2>
								<h2 style="color: white; padding-top: 12px; font-style: italic;">Costumize your plans</h2>								
							</header>
					</div>

					</div>

                    <!-- Hero Button for resulths path -->
                    

                    <div class="wrapper">
                        <section class="container">
                                              
    
                            <div class="row features">
                                
                                <% faves.forEach( book => { %>
                            
                                    <section style="text-align: center;" class="col-4 col-12-narrower feature">
                                
                                
                                        <div class="image-wrapper first">
                                            <a href="#" class="image featured"><img style='max-height: 236px;' src='<%= book.image %>' alt="" /></a>
                                        </div>
                                       <div>
                                           <p class='musicfont'><%= book.name %></p>
                                           <p class='musicfont'>Rating: <%= book.rate %></p>
                                           <p class='musicfont'><%= book.description %></p>
                                           <a  class="button" href='../books/edit/<%= book.id %>'>EDIT</a> 
                                        </div>
                                    </section>
                                    <% }) %>
                                    
                               
                                
    
                            </div>
                            <ul class="actions major">
                                <li><a href="/" class="button">Home</a></li>
                            </ul>
                        </section>
                    </div>








							<!-- Features 1 -->
				
            
				
					

				</div>

		


		</div>

		<!-- Scripts -->
			<script src="assets/js/jquery.min.js"></script>
