<html>
	<head>
		<title>Telephasic by HTML5 UP</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="assets/css/main.css" />
    <link rel="stylesheet" href="assets/css/game.css" />
    <script id="input-template" type="text/x-tmpl-mustache">
      <input type="text" name={{iteration}} placeholder='stop number {{iteration}}'>
    </script>
	</head>
	<body class="homepage is-preload">
		<div id="page-wrapper">

			<!-- Header -->
				<div id="header-wrapper">
					<div id="header" class="container">

				

						<!-- Nav -->
							<nav id="nav">
								<ul>
									<li>
										<a href="#">Dropdown</a>
										<ul>
											<li><a href="/game">Travel Game</a></li>
											<li><a href="/aboutus">About Us</a></li>
											<li><a href="/music">music</a></li>
										</ul>
									</li>
								
								</ul>
							</nav>

					</div>

					<!-- Hero -->
						<section id="hero" class="container">
							<header>
								<h2>TRAVEL APP
								<br />
								you one stop APP for awesome roadtrips</h2>
							</header>
						
							<ul class="actions">
								<li><a href="#" class="button">PLACE HOLDER FOR SEARCH</a></li>
              </ul>
              
              <form action="/search" method="post">
                <h2>Add 2 or more destinations</h2>
                <input type="text" name="start" placeholder="Seattle, WA">
                <input type="text" name="end" placeholder="New York, NY">
                <button id="add-waypoint">Add Mid Points</button>
                <button type="submit">Submit</button>
              </form>



              
              </div>

						</section>

        </div>
        
        <div class="wrapper">


        <div class="row features" style="margin-top: 2em; ">
          <h2 style='margin-left: 10%;'>Weather Forecast</h2>
        <% weatherResults.forEach(day => { %>

          
            <section class="col-12-narrower feature">
              <p class="weather">
                <%= day.time %><br>
                <%= day.forecast %>
              </p>
              <div class="image-wrapper first">
                <a href="#" class="image featured"><img id="icon" src="https://www.weatherbit.io/static/img/icons/<%= day.icon %>.png" alt="" /></a>
              </div>
              <p class="weather">
                <%= day.high_temp %>° | <%= day.low_temp %>°<br>
                Rainfall: <%= day.precip %> inches.
              </p>
            </section>
    

          
          <% }) %>
        
        </div>


  </div>

				<!-- Music 2 -->
				<div class="wrapper">
					<section class="container">
						<header class="major">
							<h2>Get some tunes for your road-trip</h2>
							<p>View the top ten albums thanks to Deezer</p>
						</header>
						<div class="row features">

						</div>
						<ul class="actions major">
							<li><a href="/music" class="button">Elevate my playlist</a></li>
						</ul>
					</section>
				</div>

			<!-- Features game -->
			<div class="wrapper" style='background: #f6f8fb'>
				
				<div class="container">
					
					<header style='background: #f6f8fb' class="major">
						<h2>Need inspiration?</h2>
						<p>Discover America: match the attraction to the city:</p>
					</header>

					<div class="row">
						<section id="memory-game"  class="memory-game">
  
							<div class="memory-card" data-framework="OR">
							  <img class="front-face" alt="Mount Calvary OR" src="images/mountcalvary.jpg" />
							  <img class="back-face" />
							  <p class="carddescription" >Mount Calvary, OR</p>
							</div>
							<div class="memory-card" data-framework="OR">
							  <img class="front-face" alt="Mirror Lake OR" src="images/mirrorlake.jpg" />
							  <img  class="back-face" />
							  <p class="carddescription" >Mirror Lake, OR</p>
							</div>
							
							<div class="memory-card" data-framework="vancouver">
							  <img class="front-face" alt="Robson Square, Vancouver" src="images/robson.jpeg" />
							  <img class="back-face" />
							  <p class="carddescription" >Robson Square, VANC</p>
							</div>
							<div class="memory-card" data-framework="vancouver">
							  <img class="front-face" alt="Sombiro Beach" src="images/sombrio.jpg" />
							  <img class="back-face" />
							  <p class="carddescription" >Sombrio Beach, VANC</p>
							</div>
							
							<div class="memory-card" data-framework="ID">
							  <img class="front-face" alt="Boise Oregon Trail Memorial" src="images/boise.jpg" />
							  <img class="back-face" />
							  <p class="carddescription" >Boise, ID</p>
							</div>
